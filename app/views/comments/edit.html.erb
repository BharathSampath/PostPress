<h1>Editing Comment</h1>


<%= form_for ([@post,@comment]) do |f| %>
    <div class="field">
      <%= f.label :content %><br />
      <%= f.text_field :content %>
    </div>
    <div class="actions">
      <%= f.submit %>
    </div>
<% end %>

<%= link_to 'All Post ', posts_path %>   |
<%= link_to 'Back to Post ', post_path(@post.id) %>


